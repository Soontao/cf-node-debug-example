{
  "name": "template",
  "version": "0.0.1",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "build": "tsc",
    "lint": "eslint src/** --fix",
    "start": "node lib/index",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "engines": {
    "node": ">=14"
  },
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/jest": "29.1.2",
    "@types/node": "16.11.66",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "eslint": "8.25.0",
    "jest": "29.2.0",
    "standard-version": "9.5.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "jest": {
    "testTimeout": 30000,
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "src/bin"
    ],
    "testEnvironment": "node",
    "testRegex": "/src/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}