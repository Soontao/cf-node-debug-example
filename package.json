{
  "name": "template",
  "version": "0.0.1",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "build": "tsc",
    "lint": "eslint src/** --fix",
    "start": "node lib/index",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "engines": {
    "node": "20",
    "npm": "10"
  },
  "license": "MIT",
  "dependencies": {
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "20.17.1",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "eslint": "^9.0.0",
    "jest": "29.7.0",
    "standard-version": "9.5.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.6.3"
  },
  "jest": {
    "testTimeout": 30000,
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "src/bin"
    ],
    "testEnvironment": "node",
    "testRegex": "/src/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
